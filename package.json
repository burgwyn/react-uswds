{
  "name": "react-uswds",
  "version": "1.0.0",
  "description": "React components based on the US Web Design Standards",
  "main": "index.js",
  "scripts": {
    "lint": "eslint ./src/**/*.jsx --quiet",
    "start": "webpack-dev-server --progress --colors --port 8181",
    "test": "jest --verbose"
  },
  "keywords": [
    "React",
    "USWDS"
  ],
  "author": "Nat Burgwyn",
  "license": "MIT",
  "homepage": "https://github.com/burgwyn/react-uswds",
  "repository": {
    "type": "git",
    "url": "https://github.com/burgwyn/react-uswds.git"
  },
  "bugs": {
    "url": "https://github.com/burgwyn/react-uswds/issues"
  },
  "peerDependencies": {
    "react": "^0.14.0 || ^15.0.0",
    "react-dom": "^0.14.0 || ^15.0.0"
  },
  "dependencies": {
    "uswds": "^0.10.0"
  },
  "devDependencies": {
    "babel-core": "^6.20.0",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^17.0.2",
    "babel-loader": "^6.2.9",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "css-loader": "^0.26.1",
    "eslint": "^3.12.2",
    "eslint-plugin-react": "^6.8.0",
    "file-loader": "^0.9.0",
    "jest-cli": "^17.0.3",
    "jsx-loader": "^0.13.2",
    "react": "^15.4.1",
    "react-addons-test-utils": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-hot-loader": "^1.3.1",
    "sass-loader": "^4.1.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.2"
  },
  "jest": {
    "collectCoverage": true,
    "testFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json"
    ],
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-dom",
      "<rootDir>/node_modules/react-addons-test-utils",
      "<rootDir>/node_modules/fbjs"
    ]
  }
}
